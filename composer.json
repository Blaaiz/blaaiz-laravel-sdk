{
  "name": "blaaiz/laravel-sdk",
  "description": "Official Laravel SDK for Blaaiz RaaS (Remittance as a Service) API",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "blaaiz",
    "payments",
    "remittance",
    "fintech",
    "api",
    "sdk",
    "laravel",
    "payout",
    "collection",
    "raas",
    "cryptocurrency",
    "bank-transfer",
    "interac",
    "open-banking"
  ],
  "authors": [
    {
      "name": "Blaaiz",
      "email": "onboarding@blaaiz.com",
      "homepage": "https://business.blaaiz.com"
    }
  ],
  "homepage": "https://docs.business.blaaiz.com",
  "support": {
    "issues": "https://github.com/blaaiz/laravel-sdk/issues",
    "source": "https://github.com/blaaiz/laravel-sdk"
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0",
    "mockery/mockery": "^1.5",
    "phpstan/phpstan": "^1.10",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Blaaiz\\LaravelSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blaaiz\\LaravelSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Blaaiz\\LaravelSdk\\BlaaizServiceProvider"
      ],
      "aliases": {
        "Blaaiz": "Blaaiz\\LaravelSdk\\Facades\\Blaaiz"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}